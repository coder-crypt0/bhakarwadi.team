{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    { "src": "/css/(.*)", "dest": "/css/$1" },
    { "src": "/js/(.*)", "dest": "/js/$1" },

    { "src": "/api/(.*)", "dest": "/api/$1" },

    { "src": "/API/(.*)", "dest": "/api/$1" },

    { "src": "/(.*)", "dest": "/api/index.php" }
  ]
}
